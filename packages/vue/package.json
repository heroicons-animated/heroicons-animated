{
  "name": "@heroicons-animated/vue",
  "version": "0.1.0",
  "description": "Animated Vue icons built with @vueuse/motion",
  "author": "Aniket-508",
  "license": "MIT",
  "homepage": "https://heroicons-animated.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/Aniket-508/heroicons-animated",
    "directory": "packages/vue"
  },
  "keywords": [
    "heroicons",
    "vue",
    "vue3",
    "icons",
    "animated",
    "vueuse",
    "motion"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./icons/*": {
      "import": "./dist/icons/*.js",
      "types": "./dist/icons/*.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "type-check": "vue-tsc --noEmit"
  },
  "peerDependencies": {
    "@heroicons-animated/shared": "workspace:*",
    "vue": "^3.0.0",
    "@vueuse/motion": "^2.0.0"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "@vitejs/plugin-vue": "^5.2.1",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-plugin-dts": "^4.3.0",
    "vue": "^3.5.13",
    "vue-tsc": "^2.1.30"
  }
}
