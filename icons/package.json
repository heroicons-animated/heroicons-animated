{
  "name": "@heroicons-animated/react",
  "version": "1.3.0",
  "description": "Animated Heroicons for React, built with Motion",
  "author": "Aniket-508",
  "license": "MIT",
  "homepage": "https://heroicons-animated.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/Aniket-508/heroicons-animated",
    "directory": "icons"
  },
  "bugs": {
    "url": "https://github.com/Aniket-508/heroicons-animated/issues"
  },
  "keywords": [
    "heroicons",
    "react",
    "icons",
    "animated",
    "motion",
    "framer-motion"
  ],
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "motion": "^12.0.0",
    "react": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.4",
    "@types/react": "^19.2.7",
    "@vitejs/plugin-react": "^4.3.4",
    "react": "^19.2.1",
    "typescript": "^5.9.3",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.3"
  }
}
